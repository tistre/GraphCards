{% extends "base.html.twig" %}

{% block body_main %}

    <h1>List of node labels</h1>

    <ul>

        {% for nodeLabel in nodeLabels %}

            <li>
                <a href="{{ path('addNode', {'label': nodeLabel}) }}">{{ nodeLabel }}</a>
            </li>

        {% endfor %}

    </ul>

    <a rel="alternate" href="{{ path('addNode') }}">Add a node</a>

{% endblock %}
